rule cc
    description = cc $in > $out
    command = /usr/bin/cc $cflags -MD -MF $out.d -c $in -o $out
    deps = gcc
    depfile = $out.d
rule link
    description = linking $out
    command = /usr/bin/cc $linkerFlags $in -o $out
