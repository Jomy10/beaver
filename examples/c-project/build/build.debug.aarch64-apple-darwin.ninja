rule cc
    description = cc $in > $out
    command = cc $cflags -MD -MF $out.d -c $in -o $out
    deps = gcc
    depfile = $out.d
rule link
    description = linking $out
    command = cc $linkerFlags $in -o $out
build build/MyProject/math/math.c.o: cc /Users/jonaseveraert/Documents/projects/Beaver4/examples/c-project/math/math.c 
    cflags = "-c" "-I/Users/jonaseveraert/Documents/projects/Beaver4/examples/c-project/math" "-g" "-O0" 
build build/MyProject/artifacts/libMyMathLibrary.a: link build/MyProject/math/math.c.o 
    linkerFlags = "-dynamiclib" "-O0" 
build MyProject$:MyMathLibrary$:staticlib: phony build/MyProject/artifacts/libMyMathLibrary.a
build build/MyProject/math/math.c.dyn.o: cc /Users/jonaseveraert/Documents/projects/Beaver4/examples/c-project/math/math.c 
    cflags = "-c" "-I/Users/jonaseveraert/Documents/projects/Beaver4/examples/c-project/math" "-g" "-O0" 
build build/MyProject/artifacts/libMyMathLibrary.dylib: link build/MyProject/math/math.c.dyn.o 
    linkerFlags = "-dynamiclib" "-O0" 
build MyProject$:MyMathLibrary$:dynlib: phony build/MyProject/artifacts/libMyMathLibrary.dylib
build MyProject$:MyMathLibrary: phony MyProject$:MyMathLibrary$:staticlib MyProject$:MyMathLibrary$:dynlib
